{
	"name": "final",
	"version": "1.0.0",
	"description": "kodilla final project",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "npm-run-all --parallel open:src lint:watch stylelint",
		"start:dist": "npm-run-all --parallel serve:dist open:dist",
		"start:local": "npm run build:local && npm run start:dist",
		"start:no-tests": "npm-run-all --parallel open:src",
		"open:src": "webpack-dev-server --env=development",
		"open:dist": "opn http://localhost:3000",
		"serve:dist": "static ./client/dist -a localhost -p 3000 --spa",
		"lint": "esw --ext .js --ext .jsx webpack.config.* client/src tools --color",
		"lint:watch": "npm run lint -- --watch",
		"stylelint": "stylelint \"client/src/**/*.scss\" --syntax scss",
		"clean-dist": "rimraf ./client/dist && mkdir client\\dist",
		"prebuild": "npm run clean-dist && npm run lint && npm run stylelint && npm run test",
		"build": "cross-env NODE_ENV=production webpack --progress --display-error-details",
		"prebuild:local": "npm run clean-dist",
		"build:local": "cross-env NODE_ENV=production webpack --env local --progress --display-error-details",
		"build:msg": "npm run clean-dist && babel client/src/**/*.msg.js && babel-node tools/translate.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Dominikanam/final.git"
	},
	"keywords": [
		"kodilla",
		"portfolio",
		"react",
		"projekt",
		"sklep",
		"final"
	],
	"author": "Dominika",
